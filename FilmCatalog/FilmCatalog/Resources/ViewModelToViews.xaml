<ResourceDictionary
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:viewmodels="clr-namespace:FilmCatalog.ViewModels"
    xmlns:views="clr-namespace:FilmCatalog.Views">

    <DataTemplate DataType="{x:Type viewmodels:UserFilmsViewModel}">
        <views:UserFilmsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type viewmodels:AuthorizationViewModel}">
        <views:AuthorizationWindow />
    </DataTemplate>

</ResourceDictionary>